    "@angular/compiler": "~4.1",
    "@angular/core": "~4.1",
    "@angular/forms": "~4.1",
    "@angular/platform-browser": "~4.1",
    "@angular/platform-browser-dynamic": "~4.1",
    "awesome-typescript-loader": "2.2.4",
    "core-js": "2.4.1",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.2.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "phantomjs-prebuilt": "2.1.13",
    "reflect-metadata": "0.1.8",
    "rimraf": "2.5.4",
    "rxjs": "^5.0.1",
    "tslint": "3.15.1",
    "typescript": "2.2.1",
    "typings": "^1.5.0",
    "webpack": "^2.0.0",
    "webpack-dev-server": "^2.0.0",
    "zone.js": "^0.8.4"
